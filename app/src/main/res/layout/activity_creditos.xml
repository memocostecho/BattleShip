<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="mx.unam.computoMovil.battleship.Creditos"
    android:background="@drawable/camufas"
    android:id="@+id/contenedorCreditos">


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="BattleFi"
        android:id="@+id/boardLabel"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:textColor="#ffffffff"
        android:textSize="40dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:text="V 1.0"
        android:id="@+id/textView2"
        android:textColor="#ffffffff"
        android:textSize="20dp"
        android:layout_below="@+id/boardLabel"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="-Bonfil De la Paz Diego"
        android:id="@+id/textView3"
        android:layout_marginTop="71dp"
        android:textColor="#ffffffff"
        android:textSize="20dp"
        android:layout_below="@+id/textView2"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="-Gomez Perez Reivaj"
        android:id="@+id/textView6"
        android:layout_alignStart="@+id/textView3"
        android:textColor="#ffffffff"
        android:textSize="20dp"
        android:layout_below="@+id/textView3"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="-Gonzalez Suaste Nayelli"
        android:id="@+id/textView7"
        android:layout_alignStart="@+id/textView6"
        android:textColor="#ffffffff"
        android:textSize="20dp"
        android:layout_below="@+id/textView6"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="-Leon Luna Christian"
        android:id="@+id/textView8"
        android:layout_alignEnd="@+id/textView6"
        android:textColor="#ffffffff"
        android:textSize="20dp"
        android:layout_below="@+id/textView7"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="-Manzano Escobar Guillermo"
        android:id="@+id/textView9"
        android:layout_alignStart="@+id/textView8"
        android:textColor="#ffffffff"
        android:textSize="20dp"
        android:layout_below="@+id/textView8"
        android:layout_centerHorizontal="true" />
</RelativeLayout>
